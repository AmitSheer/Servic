<div class="col-lg-3 col-md-6 col-sm-6" onclick="window.dispatchEvent(new Event('resize'));">
    <div class="card card-stats btn btn-primary" data-toggle="collapse" href="#collapseExample<%= index%>" style="background:white; border-color:black">
      <div class="card-header card-header-warning card-header-icon">
        <div class="card-icon" style="background: linear-gradient(
          60deg, #472682, #cd28da)">
          <i class="material-icons"><%= icon %>y</i>
        </div>
        <p class="card-category"><%= title %></p>
        <h3 class="card-title" id="<%= districtId %>">
          <span class="value"><%= value %></span>
          <small><%= unit %></small>
        </h3>
      </div>
      <div class="card-footer">
        <div class="stats">
          <i class="material-icons text-danger"><%= fotterIcon %></i>
          <a href="javascript:;"><%= fotterText %></a>
        </div>
      </div>
    </div>
  </div>